function init(self)
	self.timeout = 0
end

function fixed_update(self, dt)
	if self.timeout  > 10 then
		go.delete()
	end
	self.timeout = self.timeout + 1
	go.set_rotation(vmath.quat_rotation_z(self.timeout / 10))
	go.set_scale(vmath.vector3((10 - self.timeout) / 10, (10 - self.timeout) / 10, 1))
end	